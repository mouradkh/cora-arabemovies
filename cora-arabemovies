        var strDateTime = "Fri, 15 Oct 2015 "+time+":00 GMT";


var myDate = new Date(strDateTime);
//var GMT_to_Local= myDate.toLocaleString();

function formatAMPM(date) {
  var hours = date.getHours();
  var minutes = date.getMinutes();
  var ampm = hours >= 12 ? 'pm' : 'am';
  hours = hours % 12;
  hours = hours ? hours : 12; // the hour '0' should be '12'
  minutes = minutes < 10 ? '0'+minutes : minutes;
  var strTime = hours + ':' + minutes + ' ' + ampm;
  return strTime;
}

var GMT_to_Local = formatAMPM(myDate);

var regex = /([01]?[0-9]|2[0-3]):[0-5][0-9]/;
var local_time = GMT_to_Local.match(regex);
var regex2 = /PM|AM/i;
var local_12h = GMT_to_Local.match(regex2);

    if (typeof document.getElementsByClassName !== 'function') {
    getElementsByClassName(document.body,'time_style')[i].innerText = local_time[0];
    getElementsByClassName(document.body,'local_12h')[i].innerText = local_12h[0];
    }
    else
    {
    document.getElementsByClassName("time_style")[i].innerHTML = local_time[0];
    document.getElementsByClassName("local_12h")[i].innerHTML = local_12h[0];
    }

}

